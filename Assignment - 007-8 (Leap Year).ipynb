     "text": [
      "Please enter a year: 2000\n",
      "2000 is a leap year\n"
     ]
    }
   ],
   "source": [
    "def leap(x):\n",
    "    if x % 4 == 0 and x % 100 != 0:\n",
    "        print(f\"{x} is a leap year\")\n",
    "    elif x % 4 == 0 and x % 100 == 0 and x % 400 == 0:\n",
    "        print(f\"{x} is a leap year\")\n",
    "    else:\n",
    "        print(f\"{x} is not a leap year\")\n",
    "\n",
    "x = int(input(\"Please enter a year: \"))\n",
    "leap(x)"
   ]
  }
